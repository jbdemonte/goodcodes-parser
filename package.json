        "coveralls": "^3.0.6",
        "jest": "^24.8.0",
        "nodemon": "^1.19.1",
        "prettier": "^1.18.2",
        "ts-jest": "^24.0.2",
        "ts-node": "^8.3.0",
        "tslint": "^5.18.0",
        "tslint-config-prettier": "^1.18.0",
